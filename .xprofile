# ------ EXPORT ------
# --- LANG(X11)
export LANG='ja_JP.UTF-8'
# --- Input Method Framework
export GTK_IM_MODULE='fcitx'
export QT_IM_MODULE='fcitx'
export XMODIFIERS='@im=fcitx'
export DefaultIMModule='fcitx'
# --- DESKTOP
#export DE='xmonad'

# ------ START ------
# --- Input Method Framework(fcitx)
fcitx autostart&

# --- XKB(X Keyboard extension)
sh ~/bin/xkb.sh make
sh ~/bin/xkb.sh load
sh ~/bin/xkb.sh map

# --- Feh
/bin/feh --randomize --bg-fill ~/bin/feh/wallpaper/*

# --- Xbindkeys
xbindkeys &

# --- Xcompmgr
xcompmgr -c &

# --- Xset
sh ~/bin/xset.sh on 120

# --- Xinput(usbtouchscreen)
sh ~/bin/xrandr.sh mirror
sh ~/bin/xrandr.sh xinput

# --- Xmodmap
sh ~/bin/xmodmap.sh make
sh ~/bin/xmodmap.sh load

# --- dzen
sh ~/bin/dzen/dzconky.sh

# --- Dropbox
/usr/bin/dropbox &

# --- Window Manager(Xmonad)
exec xmonad

# ------ NOT USING ------
# Input Method Framework(Uim)
