# LANG(X11)
export LANG=ja_JP.UTF-8

# Xset
sh ~/bin/xset.sh on 60

# XKB(X Keyboard extension)
sh ~/bin/xkb.sh make
sh ~/bin/xkb.sh load

# Xbindkeys
xbindkeys &

# Xcompmgr
xcompmgr -c &

# Feh
feh --randomize --bg-fill ~/wallpaper/*

# Xmodmap
sh ~/bin/xmodmap.sh make
sh ~/bin/xmodmap.sh load

# Xmonad
exec xmonad
